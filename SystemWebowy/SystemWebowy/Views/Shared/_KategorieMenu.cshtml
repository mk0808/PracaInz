@model IEnumerable<SystemWebowy.Models.Kategoria>

<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-birthday-cake fa-fw"></i> Kategorie</a>

    <ul class="dropdown-menu">
       @foreach (var kategoria in Model)
            {
        <li>
            <a href="@Url.Action("Lista","Produkty", new { nazwaKategorii = kategoria.NazwaKategorii.ToString() })">
                @kategoria.NazwaKategorii
            </a>
        </li>
       }
    </ul>
</li>